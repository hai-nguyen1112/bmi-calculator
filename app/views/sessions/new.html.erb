<h1>Log In Form</h1>
<div class="error"><%=flash.notice%></div>
<%=form_tag '/login' do%>
<%=label :username, 'Username: '%>
<%=text_field_tag :username%><br><br>
<%=label :password, 'Password: '%>
<%=password_field_tag :password%><br>
<%=submit_tag 'Log In'%><br>
<%end%>
<%=link_to "Or create an account", new_user_url%>
